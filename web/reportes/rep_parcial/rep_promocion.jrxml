	<queryString>
		<![CDATA[SELECT e.alu_apellidos||' '||e.alu_nombres as nombres,round(avg(inf_nota),0) as promedio,c.asi_nombre,g.cur_anio FROM informe_quimestre a
inner join distributivomxc b on a.dis_codigo=b.dis_codigo
inner join asignaturas c on b.asi_codigo=c.asi_codigo
inner join matricula d on a.mat_codigo=d.mat_codigo
inner join alumnos e on d.alu_codigo=e.alu_codigo
inner join crear_curso f on d.cre_codigo=f.cre_codigo
inner join cursos g on f.cur_codigo=g.cur_codigo
where a.mat_codigo=$P{matricula}
group by e.alu_apellidos||' '||e.alu_nombres,c.asi_nombre,g.cur_anio]]>
	</queryString>
