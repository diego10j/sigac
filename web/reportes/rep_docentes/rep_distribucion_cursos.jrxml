	<queryString>
		<![CDATA[SELECT a.cre_codigo,b.per_codigo,b.per_nombre,a.cre_observacion,d.pararelo,c.docentes from crear_curso a
LEFT JOIN (
SELECT per_codigo,per_nombre FROM periodo_lectivo
)b on b.per_codigo=a.per_codigo
LEFT JOIN (
SELECT doc_codigo,doc_nombres as docentes FROM  docentes
) c on c.doc_codigo=a.doc_codigo
